---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for post in site.posts %}
  <url>
    <loc>https://beatletech.com{{ post.url }}</loc>
    {% if post.updated %}
    <lastmod>{{ post.updated | date: "%Y-%m-%d" }}</lastmod>
    {% else %}
    <lastmod>{{ post.date | date: "%Y-%m-%d" }}</lastmod>
    {% endif %}
  </url>
  {% endfor %}
  {% for post in site.case_collections %}
  <url>
    <loc>https://beatletech.com{{ post.url }}</loc>
    {% if post.updated %}
    <lastmod>{{ post.updated | date: "%Y-%m-%d" }}</lastmod>
    {% else %}
    <lastmod>{{ post.date | date: "%Y-%m-%d" }}</lastmod>
    {% endif %}
  </url>
  {% endfor %}
  <url>
    <loc>https://beatletech.com</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
  </url>
  <url>
    <loc>https://beatletech.com/cases.html</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
  </url>
  <url>
    <loc>https://beatletech.com/setup.html</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
  </url>
  <url>
    <loc>https://beatletech.com/blog.html</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
  </url>
  <url>
    <loc>https://beatletech.com/archive.html</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
  </url>
</urlset>
